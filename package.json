{
  "name": "phone-recipes",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run heroku-init",
    "make-prod": "node ./make_PROD.js && npm run make-rollup",
    "heroku-init": "npm run make-prod && cross-env NODE_ENV=production node server-app/heroku-start.js",
    "dev": "node ./make_DEV && node server-app/index.js",
    "prod": "npm run make-prod && npm run server-start",
    "make-rollup": "vite build && vite build --ssr",
    "server-start": "cross-env NODE_ENV=production node server-app/index.js",
    "test": "npx vitest /test-jsx-browser",
    "show-storybook": "node ./make_DEV && start-storybook -p 6006",
    "upload-storybook": "node ./make_PROD && build-storybook && npx chromatic --exit-zero-on-changes -d storybook-static/"
  },
